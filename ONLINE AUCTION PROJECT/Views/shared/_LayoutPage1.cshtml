                                                <li><a href="/Items/Beds">Beds</a></li>
                                                <li><a href="/Items/Tables">Table</a></li>
                                                <li><a href="/Items/WoodenCupboard">Wardrobe</a></li>

                                            </ul>
                                        </li>
                                    </ul>
                                </li>
                                <li><a href="/auction/SignIn">SIGN IN</a></li>
                                <li><a style="border:thin; background-color:red; color:white;"  href="/auction/SignUp">SIGN UP</a></li>
                            </ul>

                            <!-- Search Form -->
                         
                        <!-- Nav End -->
                    </div>
                </nav>
            </div>
        </div>
